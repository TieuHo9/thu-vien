
entity ChuyenCongTac {
	maChuyenDi String,
    tenChuyenDi String,
    thoiGianTu LocalDate,
    thoiGianDen LocalDate,
    maNhanVien String, //nguoi dai dien
    tenNhanVien String, // ten nguoi dai dien
    soDienThoai String,
    diaDiem String,
    soTien String,
}

entity NhanVien {
	maNhanVien String,
	tenNhanVien String,
    phongBan String,
    capBac String,
    
}

entity NhanVienCongTac{
	maNhanVien String,
    maChuyenDi String,
    soTien String,
}

entity PhongBanNhanVien{
	maPhong String,
    maNhanVien String,
}

// an ignored comment
