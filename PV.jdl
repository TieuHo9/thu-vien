}

relationship ManyToMany {
	ChuyenCongTac{nhanvien} to NhanVien{chuyencongtac},
    DeXuatThanhToan{chiphi} to ChiPhi{dexuat},
    DeXuatThanhToan{dmuc} to DinhMuc{dxuattt}
    NhanVien{phongban} to PhongBan{nhanv},
    NhanVien{capbac} to CapBac{nv},
    NhanVienCongTac{chuyenct} to ChuyenCongTac{nhanvienct},
    PhongBanNhanVien{nvien} to NhanVien{pban},
    DinhMuc{cbac} to CapBac{muc}
    
    
}